<AnchorPane prefHeight="536.0" prefWidth="1085.0" style="-fx-background-color: #f3f4f6;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UserTableViewController">
