<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="602.0" prefWidth="1080.0" style="-fx-background-color: #f3f4f6;"
   xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/22"
   fx:controller="controller.UserTableViewController">
   <children>
      <TableView fx:id="userTable" layoutX="193.0" layoutY="282.0" prefHeight="312.0"
         prefWidth="844.0" style="-fx-background-color: #f9fafb; -fx-border-color: #d1d5db;">
         <columns>
            <TableColumn fx:id="idUserColumn" text="ID User" />
            <TableColumn fx:id="nameColumn" text="Name" />
            <TableColumn fx:id="phoneNumberColumn" prefWidth="118.0" text="Phone Number" />
            <TableColumn fx:id="emailColumn" prefWidth="180.0" text="Email" />
            <TableColumn fx:id="avatarColumn" prefWidth="104.0" text="Avatar" />
            <TableColumn fx:id="statusColumn" prefWidth="90.0" text="Status" />
            <TableColumn fx:id="roleColumn" prefWidth="98.0" text="Role" />
            <TableColumn fx:id="actionColumn" prefWidth="92.0" text="Action" />
         </columns>
         <cursor>
            <Cursor fx:constant="W_RESIZE" />
         </cursor>
      </TableView>
      <Pane prefHeight="602.0" prefWidth="170.0" style="-fx-background-color: #f9fafb;">
         <children>
            <ImageView fx:id="logoConcatoo" fitHeight="119.0" fitWidth="121.0" layoutX="25.0"
               layoutY="14.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/logo.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
   </children>
</AnchorPane>