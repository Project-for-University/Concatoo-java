<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="536.0" prefWidth="1085.0" style="-fx-background-color: #f3f4f6;"
    xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="controller.DashboardController">
    <children>
        <Pane prefHeight="535.0" prefWidth="170.0"
            style="-fx-background-color: #f9fafb; -fx-border-color: #d1d5db;">
            <children>
                <ImageView fx:id="logoConcatoo" fitHeight="75.0" fitWidth="80.0" layoutX="46.0"
                    layoutY="30.0" nodeOrientation="INHERIT" pickOnBounds="true"
                    preserveRatio="true">
                    <image>
                        <Image url="@/images/logo.png" />
                    </image>
                </ImageView>
                <Button fx:id="listUserButton" layoutX="25.0" layoutY="255.0"
                    mnemonicParsing="false" prefHeight="25.0" prefWidth="121.0" text="List User" />
                <Button fx:id="dashboardButton" layoutX="25.0" layoutY="204.0"
                    mnemonicParsing="false" prefHeight="25.0" prefWidth="121.0" text="Dashboard" />
            </children>
        </Pane>
        <Pane layoutX="203.0" layoutY="35.0" prefHeight="119.0" prefWidth="202.0"
            style="-fx-border-color: #d1d5db; -fx-border-radius: 10; -fx-background-color: #f9fafb;">
            <children>
                <Label layoutX="25.0" layoutY="14.0" text="Total User">
                    <font>
                        <Font name="Montserrat Regular" size="15.0" />
                    </font>
                </Label>
                <Label fx:id="LTotalUser" layoutX="25.0" layoutY="40.0" text="0">
                    <font>
                        <Font name="Montserrat Bold" size="40.0" />
                    </font>
                </Label>
            </children>
        </Pane>
    </children>
</AnchorPane>